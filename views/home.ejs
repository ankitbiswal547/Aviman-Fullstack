<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Aviman</title>
    <link rel="icon"
        href="https://res.cloudinary.com/www-aviman-com/image/upload/v1630854560/Aviman/assets/AVIMAN_2_ekquhz.png">
    <!-- STYLESHEETS -->
    <link rel="stylesheet" href="/stylesheets/styles.css">
    <link rel="stylesheet" href="/stylesheets/aboutUs.css">
    <link rel="stylesheet" href="/stylesheets/categories.css">
    <link rel="stylesheet" href="/stylesheets/newProducts.css">
    <link rel="stylesheet" href="/stylesheets/contactUs.css">
    <link rel="stylesheet" href="/stylesheets/categoryProducts/categoryProducts.css">
    <link rel="stylesheet" href="/stylesheets/flash/flash.css">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;200;300;400;500;600;700;800;900&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"
        integrity="sha512-iBBXm8fW90+nuLcSKlbmrPcLa0OT92xO1BIsZ+ywDWZCvqsWgccV3gFoRBv0z+8dLJgyAHIhR35VZc2oM/gI1w=="
        crossorigin="anonymous" />
</head>

<body>
    <%- include('partials/flashMsg') %>
        <%- include('partials/navbar') %>


            <div id="first">
                <div class="carousel">
                    <i id="pauseSlideshow" class="fas fa-pause"></i>
                    <i id="resumeSlideshow" class="fas fa-play"></i>
                    <i id="prevBtn" class="fas fa-chevron-left"></i>
                    <i id="nextBtn" class="fas fa-chevron-right"></i>
                    <div class="carousel-slide">
                        <img class="carousel-images"
                            src="https://res.cloudinary.com/www-aviman-com/image/upload/v1630854478/Aviman/assets/img5_olly91.webp"
                            id="last-clone" alt="">
                        <img class="carousel-images"
                            src="https://res.cloudinary.com/www-aviman-com/image/upload/v1630854471/Aviman/assets/img1_v3lwfr.webp"
                            alt="">
                        <img class="carousel-images"
                            src="https://res.cloudinary.com/www-aviman-com/image/upload/v1630854467/Aviman/assets/img2_rb5uny.webp"
                            alt="">
                        <img class="carousel-images"
                            src="https://res.cloudinary.com/www-aviman-com/image/upload/v1630854468/Aviman/assets/img3_bq0mei.webp"
                            alt="">
                        <img class="carousel-images"
                            src="https://res.cloudinary.com/www-aviman-com/image/upload/v1630854468/Aviman/assets/img4_o5arno.webp"
                            alt="">
                        <img class="carousel-images"
                            src="https://res.cloudinary.com/www-aviman-com/image/upload/v1630854478/Aviman/assets/img5_olly91.webp"
                            alt="">
                        <img class="carousel-images"
                            src="https://res.cloudinary.com/www-aviman-com/image/upload/v1630854471/Aviman/assets/img1_v3lwfr.webp"
                            id="first-clone" alt="">
                    </div>
                </div>

            </div>


            <!-- STORE -->
            <div id="second">
                <h2 class="div-title">WHAT'S IN THE STORE</h2>
                <div class="container">

                    <% for(let product of products){ %>
                        <div class="product">
                            <a href="/products/<%= product._id %>">
                                <!-- <span class="pdt-floating-span">
                                <button class="pdt-floating-span-btn-1">View More</button>
                                <button onclick="showSnackbar()" class="pdt-floating-span-btn-2">Add To Cart</button>
                            </span> -->
                                <div class="image-part">
                                    <img src="<%= product.productImages[0].url %>" alt="">
                                    <% if(product.isOnSale) {%>
                                        <div class="percentage-off-sec">
                                            <div>
                                                <%= percentageOff %>%
                                            </div>
                                            <div>OFF</div>
                                        </div>
                                        <% } %>
                                </div>
                                <div class="product-desc">
                                    <h3>
                                        <%= product.productName %>
                                    </h3>
                                    <div class="product-pricing">
                                        <div class="pricing-sec">
                                            <% if(product.isOnSale) {%>
                                                <p class="original-price pdt-on-sale">Rs. <%= product.price %>
                                                </p>
                                                <p class="offer-price">Rs. <%= product.discountPrice %>
                                                </p>
                                                <% }else{ %>
                                                    <p class="original-price">Rs. <%= product.price %>
                                                    </p>
                                                    <% } %>
                                        </div>
                                    </div>
                                    <div class="free-shipping-msg"><i class="fas fa-tag"></i> Free Shipping Across India
                                    </div>
                                </div>
                            </a>
                        </div>
                        <% } %>
                </div>
                <div class="second-btn-area">
                    <a href="/products" class="second-view-more-btn">View More</a>
                </div>
            </div>


